{
	"name": "lint",
	"version": "0.1.0",
	"description": "",
	"license": "MIT",
	"scripts": {
		"build": "/usr/local/bin/node node_modules/.bin/webpack --mode=production",
		"build:watch": "/usr/local/bin/node node_modules/.bin/webpack --mode=development --watch",
		"prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}' "
	},
	"dependencies": {
		"classnames": "^2.3.2",
		"framer-motion": "^10.12.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@figma/plugin-typings": "^1.64.0",
		"@types/react": "^18.2.6",
		"@types/react-dom": "^18.2.4",
		"css-loader": "^6.7.3",
		"html-webpack-inline-source-plugin": "^0.0.10",
		"html-webpack-plugin": "^5.5.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"style-loader": "^3.3.2",
		"ts-loader": "^9.4.2",
		"tslint": "^5.20.1",
		"tslint-react": "^4.1.0",
		"typescript": "^5.0.4",
		"url-loader": "^4.1.1",
		"webpack": "^5.82.0",
		"webpack-cli": "^5.1.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
	}
}
